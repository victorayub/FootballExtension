{
  "manifest_version": 2,

  "name": "Champions Results",
  "description": "This extension shows the results of the Champions League last games",
  "version": "1.0",

  "browser_action": {
    "default_icon": "champions_icon.png",
    "default_popup": "popup.html",
    "default_title": "See results"
  },
  "permissions": [
    "http://api.football-data.org/*"
  ]
}